{
    "private": true,
    "scripts": {
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "concurrently \"cross-env RENDER_TARGET=server npm run development -- --watch\" \"cross-env RENDER_TARGET=client npm run development -- --watch\"",
        "dev": "concurrently \"cross-env RENDER_TARGET=server npm run development\" \"cross-env RENDER_TARGET=client npm run development\"",
        "prod": "git clean -xdf public && concurrently \"cross-env RENDER_TARGET=server npm run production\" \"cross-env RENDER_TARGET=client npm run production\" && tar zcvf public.tar.gz public/css public/js public/mix-manifest.json"
    },
    "devDependencies": {
        "@vue/compiler-sfc": "^3.1.4",
        "babel-plugin-import": "^1.13.3",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.2.2",
        "concurrently": "^6.2.0",
        "cross-env": "^7.0",
        "laravel-mix": "*",
        "lodash": "^4.17.19",
        "portal-vue": "^2.1.7",
        "postcss-import": "^12.0.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue-loader": "^16.2.0"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.0.1",
        "ant-design-vue": "^2.2.1",
        "axios": "^0.21.1",
        "chart.js": "^3.4.1",
        "vue": "^3.1.4",
        "vue-chart-3": "^0.3.3",
        "vue-progressbar": "^0.7.5",
        "vue-router": "^4.0.10",
        "vue-screen": "^2.0.3-alpha.3",
        "vue-server-renderer": "^2.6.14"
    }
}
